{
    "agent_parameters" :
    {
        "num_agents" : 100,
        "new_machines_after_mutation": 25,

        "machine_parameters" :
        {
            "behavior" : "selfish",
            "observed_span" : 300,
            "observation_type" : "previous_agents_plus_start_time_detector_data",
            "group_vicinity": false
        },

        "human_parameters" :
        {
            "model" : "gawron",

            "noise_weight_agent" : 0.2,
            "noise_weight_path" : 0.6,
            "noise_weight_day" : 0.2,

            "beta" : -1.0,
            "beta_k_i_variability" : 0.0,
            "epsilon_i_variability" : 0.0,
            "epsilon_k_i_variability" : 0.0,
            "epsilon_k_i_t_variability" : 0.0,

            "greedy" : 1.0,
            "gamma_c" : 0.0,
            "gamma_u" : 0.0,
            "remember" : 1,

            "alpha_zero" : 0.2,
            "alphas" : [0.8]  
        },

        "simulation_timesteps" : "${simulator_parameters.simulation_timesteps}",
        "origins" : "${path_generation_parameters.origins}",
        "destinations" : "${path_generation_parameters.destinations}",
        "action_space_size" : "${path_generation_parameters.number_of_paths}",
        "records_folder" : "${plotter_parameters.records_folder}"
    },

    "environment_parameters":
    {
        "number_of_days" : 1,

        "action_space_size" : "${path_generation_parameters.number_of_paths}"
    },

    "simulator_parameters":
    {
        "network_name" : "csomor",
        "simulation_timesteps" : 180,
        "sumo_type" : "sumo",
        "stuck_time" : 600,

        "number_of_paths" : "${path_generation_parameters.number_of_paths}",
        "plots_folder": "${plotter_parameters.plots_folder}",
        "records_folder" : "${plotter_parameters.records_folder}"
    },
        
    "path_generation_parameters":
    {
        "number_of_paths" : 3,
        "beta" : -3,
        "weight" : "time",
        "num_samples" : 100,
        "visualize_paths" : true,

        "origins" : "default",
        "destinations" : "default",

        "records_folder" : "${plotter_parameters.records_folder}"
    }, 

    "plotter_parameters":
    {
        "records_folder": "training_records",
        "plots_folder": "plots",

        "plot_choices" : [true, true, true, true, true, true, true],
        "smooth_by" : 50,
        "phases" : [0, 100],
        "phase_names" : [
            "Human learning", 
            "Mutation - Machine learning"
        ]
    }
}